    <%= form_for(@picture) do |f| %>
      <%= f.error_messages! %>
      <div class="field">
        <%= f.text_field :title, autofocus: true, class: "form-control", placeholder: "タイトル" %>
      </div>

      <div class="field">
         <%= f.file_field :image %>
      </div>
      <%= f.submit %>
    <% end %>
    <%= link_to "一覧画面にもどる", pictures_path %>
